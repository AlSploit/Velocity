repeat task.wait() until game:IsLoaded()

local LocalPlayer = game.Players.LocalPlayer

local VelocityLoaded = shared.VelocityLoaded
local MatchingKey = loadstring(game:HttpGet("https://raw.githubusercontent.com/AlSploit/Velocity/main/SecurityCheck"))()
local SecretKey = "5OKzZB1QFOthIQz26K9AL6woKN5CpWAIPZHKBptEeXpfhHCoi1JiCAAqSIDFj8bB8a48uB9WjuM5E8mbpsaGCM2GB705stgEE5z6"

VelocityLoaded = false

function Kick(Message)
	LocalPlayer:Kick(Message)

	task.wait(5)

	game:shutdown()	
end

task.spawn(function() --Key check
	if makefolder and writefile and readfile and isfile then
		task.spawn(function()
			if MatchingKey == true then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/AlSploit/Velocity/main/MainScript"))()	
				
				VelocityLoaded = true
			end
		end)
	end
end)

task.spawn(function()
	if MatchingKey == SecretKey or MatchingKey == false or MatchingKey == nil then
		Kick("Velocity is currently not working.")
	end
end)

task.spawn(function() --Executor check
	if not makefolder then
		Kick("Your executor does not support Velocity.")
	end
	
	if not writefile then
		Kick("Your executor does not support Velocity.")
	end
	
	if not readfile then
		Kick("Your executor does not support Velocity.")
	end
	
	if not isfile then
		Kick("Your executor does not support Velocity.")
	end
end)
